@{
    ViewData["Title"] = "Home Page";
}

<div id="waveform"></div>

<script src="https://unpkg.com/wavesurfer.js"></script>
<script>
    var wavesurfer = WaveSurfer.create({
        container: '#waveform',
        waveColor: 'violet',
        progressColor: 'purple'
    });
    wavesurfer.load('fete.wav');
    wavesurfer.on('ready', function () {
        wavesurfer.play();
    });
</script>
